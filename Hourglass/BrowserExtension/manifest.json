{
  "manifest_version": 3,
  "name": "App Limiter Browser Extension",
  "version": "1.0",
  "description": "Tracks browser tab URLs for app usage limiting",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "http://localhost:*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}